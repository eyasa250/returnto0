<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.returnto0.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_task"
        android:name="com.example.returnto0.ui.task.TaskFragment"
        tools:layout="@layout/fragment_task" />
    <fragment
        android:id="@+id/navigation_addEvent"
        android:name="com.example.returnto0.ui.event.addeventFragment"
        tools:layout="@layout/fragment_addevent" />
    <fragment
        android:id="@+id/navigation_room"
        android:name="com.example.returnto0.ui.Room.Roomfragment"
        android:label="@string/title_todolist"
        tools:layout="@layout/fragment_room_list" />


    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.returnto0.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notification" />

</navigation>